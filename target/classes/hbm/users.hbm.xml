<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.tastydine.vo.User" table="login_users">  
    <id name="id">  
     <generator class="increment"></generator>  
    </id>  
    <property name="userName" column="user_name" type="string"></property>  
    <property name="password" column="password" type="string"></property>
    <property name="createdTime" column="created_time" type="date"></property>
    <property name="modifiedTime" column="modified_time" type="date"></property>  
  </class>  
 </hibernate-mapping>  